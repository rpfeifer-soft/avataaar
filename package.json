  "license": "MIT",
  "dependencies": {
    "canvas": "^2.6.1",
    "canvg": "^3.0.7",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "xmldom": "^0.4.0"
  }
}
